{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://raw.githubusercontent.com/hen2026/sao/main/sao_antrag-i-schema.json",
  "type": "object",
  "additionalProperties": true,
  "required": ["submitter"],
  "properties": {
    "submitter": {
      "type": "object",
      "additionalProperties": true,
      "required": [
        "DF-PSB1-01",
        "DF-PSB1-02",
        "DF-PSB1-03",
        "DF-PSB1-04"
      ],
      "properties": {
        "DF-PSB1-01": { "type": "string" },
        "DF-PSB1-02": { "type": "string" },
        "DF-PSB1-03": { "type": "string" },
        "DF-PSB1-04": { "type": "string" },

        "DF-ADR-01": { "type": "string" },
        "DF-ADR-02": { "type": "string" },
        "DF-ADR-03": { "type": "string" },
        "DF-ADR-04": { "type": "string", "pattern": "^[0-9]+$" },
        "DF-ADR-05": { "type": "string" },
        "DF-ADR-06": { "type": "string" },

        "DF-KON-01": { "type": "string" },
        "DF-KON-02": { "type": ["string", "null"] },
        "DF-KON-03": { "type": "string" },

        "DF-KIND-01": { "type": "string" },
        "DF-KIND-02": { "type": "string" },
        "DF-KIND-03": { "type": "string" },

        "DF-ADR-KIND-01": { "type": ["string", "null"] },
        "DF-ADR-KIND-02": { "type": ["string", "null"] },
        "DF-ADR-KIND-03": { "type": ["string", "null"] },
        "DF-ADR-KIND-04": { "type": ["string", "null"] },
        "DF-ADR-KIND-05": { "type": ["string", "null"] },
        "DF-ADR-KIND-06": { "type": ["string", "null"] },

        "DF-SR-01": { "type": ["string", "null"] },
        "DF-SR-02": { "type": ["string", "null"] },
        "DF-SR-03": { "type": ["string", "null"] },
        "DF-SR-04": { "type": ["string", "null"] },
        "DF-SR-05": { "type": ["string", "null"] },
        "DF-SR-06": { "type": ["string", "null"] },
        "DF-SR-07": { "type": ["string", "null"] },
        "DF-SR-08": { "type": ["string", "null"] },
        "DF-SR-09": { "type": ["string", "null"] },
        "DF-SR-10": { "type": ["string", "null"] },

        "DF-ANTR-01": { "type": "boolean" },
        "DF-ANTR-02": { "type": "boolean" },
        "DF-ANTR-03": { "type": "boolean" },
        "DF-ANTR-04": { "type": ["string", "null"] },
        "DF-ANTR-05": { "type": "boolean" },
        "DF-ANTR-06": { "type": "boolean" }
      }
    }
  }
}
